    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AccountSettingsActivity"/>
        <activity android:name=".EditProfileActivity"/>
        <activity android:name=".EditPasswordActivity"/>
        <activity android:name=".ResetPasswordActivity"></activity>
        <activity android:name=".SignUpActivity"/>
        <activity android:name=".MenuDetailActivity"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".ConsultationAcitivity"/>
        <activity android:name=".ConsultationResultActivity"/>
        <activity android:name=".ConsultationDetailActivity"/>
        <activity android:name=".TipsDetailActivity"></activity>
    </application>

</manifest>